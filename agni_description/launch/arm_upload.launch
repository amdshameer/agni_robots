<launch>
	<!-- load description of the arm into robot_description -->
	<arg name="namespace" default="ra"/>
  <arg name="arm_type" default="kuka"/>
  <arg name="reflect" default="1"/>
	
  <!-- load robot_description -->
  <param name="robot_description" command="$(find xacro)/xacro.py '$(find agni_description)/robots/$(arg arm_type)_gazebo.urdf.xacro namespace:=$(arg namespace) reflect:=$(arg reflect)'" />
</launch>
